@page
@model EksamenProjekt2Sem.Pages.LogInAndOut.LogInPageModel
@{
	<div class="container">
		<div>
			@Model.Message
		</div>
		<form method="post">
			<div class="form-group">
				<label asp-for="Email" class="col-form-label ">E-mail</label>
				<div>
					<input asp-for="Email" />
				</div>
			</div>
			
			<div class="form-group">
				<label asp-for="Password" class="col-form-label">Adgangskode</label>
				<div>
					<input asp-for="Password" />
				</div>
			</div>
			<br />
			<div class="form-group">
				<button class="btn btn-outline-dark">Log ind</button>
			</div>
			<br />
			<br />
			<br />
			<div class="form-group">
				<p>Har du ikke en bruger endnu? <a asp-page="/User/CreateUser">Opret dig her!</a></p>
			</div>
		</form>
	</div>
}


